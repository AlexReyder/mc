generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  user_id       Int    @unique(map: "user_id")
  user_login    String @db.VarChar(30)
  user_password String @db.VarChar(32)
  user_hash     String @db.VarChar(32)
  user_email    String @db.Text
}
